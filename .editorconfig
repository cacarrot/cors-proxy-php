# https://editorconfig.org/

root = true

[*]
trim_trailing_whitespace = true
insert_final_newline     = true
end_of_line              = lf
charset                  = utf-8

[*.{php}]
indent_size              = 4
indent_style             = space

[*.{yml}]
indent_size              = 2
indent_style             = space
